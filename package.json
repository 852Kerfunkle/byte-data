{
  "name": "byte-data",
  "version": "4.0.3",
  "description": "Read and write data to and from byte buffers.",
  "homepage": "https://github.com/rochars/byte-data",
  "author": "Rafael da Silva Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "keywords": [
    "byte",
    "binary",
    "array",
    "int",
    "integer",
    "signed",
    "unsigned",
    "float",
    "half-precision",
    "single-precision",
    "double-precision",
    "IEEE",
    "char",
    "fourCC",
    "string",
    "boolean",
    "crumb",
    "nibble",
    "1-bit",
    "2-bit",
    "4-bit",
    "8-bit",
    "16-bit",
    "24-bit",
    "32-bit",
    "40-bit",
    "48-bit",
    "64-bit",
    "struct",
    "pack",
    "unpack",
    "endian",
    "big-endian",
    "little-endian"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/byte-data.git"
  },
  "scripts": {
    "lint": "jshint index.js && jshint src",
    "test": "nyc ./node_modules/mocha/bin/_mocha test/src --recursive --src",
    "test-dist": "nyc ./node_modules/mocha/bin/_mocha test/src --recursive --dist",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "bundle": "webpack",
    "compile": "google-closure-compiler-js dist/byte-data.js > dist/byte-data-min.js --compilationLevel=ADVANCED",
    "doc": "./node_modules/.bin/jsdoc index.js src/type.js -d docs -t node_modules/docdash -r README.md",
    "pack": "npm run bundle && npm run compile && npm run test-dist && npm run doc",
    "build": "npm run lint && npm run pack && npm test"
  },
  "devDependencies": {
    "browser-env": "^3.2.4",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "docdash": "^0.4.0",
    "google-closure-compiler-js": "^20170910.0.1",
    "jsdoc": "~3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^3.2.2",
    "string-replace-loader": "^1.3.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "endianness": "^3.0.1"
  },
  "main": "index.js"
}
