{
  "name": "byte-data",
  "version": "11.0.0-alpha.0",
  "description": "Pack and unpack binary data.",
  "homepage": "https://github.com/rochars/byte-data",
  "author": "Rafael S. Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "main": "./dist/byte-data.cjs.js",
  "module": "./main.js",
  "browser": "./dist/byte-data.umd.js",
  "jsdelivr": "./dist/byte-data.min.js",
  "unpkg": "./dist/byte-data.min.js",
  "engines": {
    "node": ">=7"
  },
  "keywords": [
    "byte",
    "binary",
    "struct",
    "pack",
    "unpack",
    "big-endian",
    "little-endian",
    "network order",
    "integer",
    "unsigned",
    "signed",
    "two's complement",
    "float",
    "IEEE",
    "half-precision",
    "single-precision",
    "double-precision",
    "char",
    "fourCC",
    "string",
    "boolean",
    "crumb",
    "nibble",
    "1-bit",
    "2-bit",
    "4-bit",
    "8-bit",
    "16-bit",
    "24-bit",
    "32-bit",
    "40-bit",
    "48-bit",
    "64-bit"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/byte-data.git"
  },
  "bugs": {
    "url": "https://github.com/rochars/byte-data/issues"
  },
  "scripts": {
    "lint": "jshint main.js ./lib/types.js ./lib/packer.js ./lib/integer.js",
    "test": "node -r esm ./node_modules/mocha/bin/_mocha test/src --recursive",
    "test-min": "nyc ./node_modules/mocha/bin/_mocha test/src --recursive --min",
    "test-cjs": "nyc ./node_modules/mocha/bin/_mocha test/src --recursive --cjs",
    "test-umd": "nyc ./node_modules/mocha/bin/_mocha test/src --recursive --umd",
    "pack": "npm test && webpack && npm run test-min && npm run test-cjs && npm run test-umd",
    "doc": "./node_modules/.bin/jsdoc main.js -d docs -t node_modules/docdash -r README.md",
    "build": "npm run lint && npm run pack && npm run doc",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "devDependencies": {
    "browser-env": "^3.2.4",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "docdash": "^0.4.0",
    "esm": "^3.0.51",
    "google-closure-compiler-js": "^20180610.0.0",
    "jsdoc": "~3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^3.2.2",
    "webpack": "^3.8.0"
  },
  "dependencies": {
    "endianness": "6.0.0-alpha.2"
  }
}
