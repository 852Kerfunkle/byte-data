{
  "name": "byte-data",
  "version": "0.0.8",
  "description": "Bytes to and from numbers and strings.",
  "homepage": "https://github.com/rochars/byte-data",
  "author": "Rafael da Silva Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "keywords": [
    "byte",
    "array",
    "int",
    "float",
    "char",
    "string",
    "8-bit",
    "16-bit",
    "24-bit",
    "32-bit",
    "64-bit"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/byte-data.git"
  },
  "scripts": {
    "lint": "jshint index.js && jshint src",
    "test": "nyc ./node_modules/mocha/bin/_mocha --timeout=1800000",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "bundle": "webpack",
    "compile": "google-closure-compiler-js dist/byte-data.js > dist/byte-data-min.js --compilationLevel ADVANCED",
    "doc": "./node_modules/.bin/jsdoc src/to-bytes.js src/from-bytes.js -d docs",
    "build": "npm run lint && npm test && npm run bundle && npm run compile && npm run doc"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "google-closure-compiler-js": "^20170910.0.1",
    "jsdoc": "~3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^3.2.2",
    "string-replace-loader": "^1.3.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "int-bits": "^1.0.0"
  },
  "main": "index.js"
}
